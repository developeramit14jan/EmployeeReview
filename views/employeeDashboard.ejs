<link rel="stylesheet" href="/css/employeedashboard.css">
<div class="nav">
    <a href="#">Home</a>
</div>
<div id="employeeData">
    <h1>List of performance review : </h1>
    <table>
        <tr>
            <th>SNo : </th>
            <th>Name </th>
            <th>Email </th>
            <th>Action </th>
        </tr>

        <% for(var i=0; i<allEmployee.length; i++) {%>
            <tr>
                <td>
                    <%= i+1 %>
                </td>
                <td>
                    <%= allEmployee[i].name %>
                </td>
                <td>
                    <%= allEmployee[i].email %>
                </td>

                <td><button><a href="/employee/<%= allEmployee[i].id %>"> Feedback</button></td>

            </tr>
            <% } %>

    </table>
</div>
</body>